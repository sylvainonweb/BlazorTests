<CascadingAuthenticationState>
    <Router AppAssembly="typeof(Program).Assembly">
        <Found Context="routeData">
            <AuthorizeRouteView RouteData="@routeData" DefaultLayout="typeof(MainLayout)">
                <NotAuthorized>
                    <BlazorTests.Pages.Login.LoginView></BlazorTests.Pages.Login.LoginView>
            </NotAuthorized>
            </AuthorizeRouteView>
        </Found>
        <NotFound>
            <BlazorTests.Pages.Errors.Error404View></BlazorTests.Pages.Errors.Error404View>
        </NotFound>
    </Router>
</CascadingAuthenticationState>
